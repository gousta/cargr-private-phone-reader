{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "__dirname": "readonly"
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      "warn",
      "single"
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "no-console": 0
  }
}